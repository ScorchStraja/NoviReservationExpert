                    <Button Width="{Binding ActualWidth, ElementName=grdMeni}"
                        Name="btnUser"
                        Height="50"
                        Background="Transparent"
                        HorizontalAlignment="Left"
                        Margin="10 5  10 20"
                        HorizontalContentAlignment="Left"
                        VerticalContentAlignment="Center"
                        BorderBrush="Transparent"
