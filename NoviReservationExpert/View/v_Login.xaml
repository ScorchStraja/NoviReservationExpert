                                   Margin="0,0,10,0">
                                </Image>
                                <TextBlock Text="PIN LOGIN"
                                       TextWrapping="Wrap"
                                       Foreground="WhiteSmoke"
                                       FontSize="15"
                                       VerticalAlignment="Center"
                                       HorizontalAlignment="Right">
                                </TextBlock>
                            </StackPanel>
