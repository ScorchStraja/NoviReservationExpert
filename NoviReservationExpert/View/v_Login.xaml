                    Grid.Row="2"
                    Height="50"
                    VerticalAlignment="Bottom"
                    Background="Transparent"
                    HorizontalAlignment="Left"
                    HorizontalContentAlignment="Left"
                    Margin="15"
                    BorderBrush="Transparent"
