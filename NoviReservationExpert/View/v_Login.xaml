                                    FontSize="22"
                                   TextWrapping="Wrap"
                                   FontWeight="Bold"
                                    Foreground="White"
                                   HorizontalAlignment="Center"
                                    FontFamily="Bahnschrift">
                        </TextBlock>
                    </StatusBar>
                    <Separator Margin="20 0"></Separator>
                    <Button Width="{Binding ActualWidth, ElementName=grdMeni}"
                Height="50"
                Background="Transparent"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Margin="10"
                BorderBrush="Transparent"
                Command="{Binding PadajuciMeni_Command}">
                        <Button.Content>
                            <StackPanel Orientation="Horizontal" 
                                    Width="{Binding ActualWidth, ElementName=btnPin}">
                                <Image Source="/Resursi/Slike/objekti.png" 
                                   Height="50" 
                                   Width="50" 
                                   VerticalAlignment="Center"
                                   Margin="0,0,10,0">
                                </Image>
                                <TextBlock Text="OBJEKTI"
                                   x:Name="tbObjekti"
                                       Foreground="WhiteSmoke"
                                       FontSize="15"
                                       VerticalAlignment="Center"
                                       HorizontalAlignment="Right">
                            <!--<i:Interaction.Triggers>
                                <i:EventTrigger EventName="MouseLeftButtonDown">
                                    <i:InvokeCommandAction Command="{Binding PadajuciMeni_Command}"></i:InvokeCommandAction>
                                </i:EventTrigger>
                            </i:Interaction.Triggers>-->
                                </TextBlock>
                            </StackPanel>
                        </Button.Content>
                    </Button>
                    <Border x:Name="brdPadajuciMeni"
                        Width="{Binding ActualWidth, ElementName=grdMeni}"
                    Height="0"
                    Visibility="Visible">
                        <Border.Background>
                            <LinearGradientBrush StartPoint="0 0" EndPoint="0 1">
                                <LinearGradientBrush.GradientStops>
                                    <GradientStop Offset="1" Color="Transparent" />
                                    <GradientStop Offset="0.4" Color="Transparent" />
                                </LinearGradientBrush.GradientStops>
                            </LinearGradientBrush>
                        </Border.Background>

                    </Border>
                </StackPanel>

                <StackPanel Grid.Row="1"
                        VerticalAlignment="Bottom">
                    <Separator Margin="20 0"></Separator>
                    <Button Width="{Binding ActualWidth, ElementName=grdMeni}" 
                        x:Name="btnPin"
                        Height="50"
                        Background="Transparent"
                        HorizontalAlignment="Left"
                        HorizontalContentAlignment="Left"
                        VerticalContentAlignment="Center"
                        Margin="10 5 10 0"
                        BorderBrush="Transparent"
                        Command="{Binding PrikaziPinTastatura_Command}">
                        <Button.Content>
                            <StackPanel Orientation="Horizontal" 
                                    Width="{Binding ActualWidth, ElementName=btnPin}">
                                <Image Source="/Resursi/Slike/calculatorBeli.png" 
                                   Height="50" 
