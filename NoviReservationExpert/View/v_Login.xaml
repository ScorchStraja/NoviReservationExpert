                                    Width="{Binding ActualWidth, ElementName=btnUser}">
                                <Grid HorizontalAlignment="Left">
                                    <Image Source="/Resursi/Slike/userlogin.png" 
                                   Height="50" 
                                   Width="50" 
                                   Margin="0,0,10,0"
