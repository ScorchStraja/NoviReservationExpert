<Window x:Class="NoviReservationExpert.View.v_SlobodniStolovi"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NoviReservationExpert.View" xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        xmlns:i="http://schemas.microsoft.com/xaml/behaviors"   
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="Slobodni stolovi" Height="650" Width="1420">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="190"></ColumnDefinition>
            <ColumnDefinition Width="200"></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition Height="50"></RowDefinition>
        </Grid.RowDefinitions>
        <Border Grid.Column="0"                             
                >
            <DockPanel LastChildFill="False">
                <Calendar DockPanel.Dock="Top"                        
                          SelectedDate="{Binding IzabranDatum}">
                </Calendar>
                <DockPanel DockPanel.Dock="Top"
                                    Margin="5,4,5,0">
                    <TextBlock Text="Vreme:"
                                       Width="70"></TextBlock>
                    <TextBox Margin="5,0,0,0"
                             Text="{Binding IzabranoVreme, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                             HorizontalContentAlignment="Center">
                        <i:Interaction.Triggers>
                            <i:EventTrigger EventName="GotFocus">
                                <i:InvokeCommandAction Command="{Binding IzabranTB_Command}" CommandParameter="Vreme"></i:InvokeCommandAction>
                            </i:EventTrigger>
                        </i:Interaction.Triggers>
                    </TextBox>
                </DockPanel>
                <DockPanel DockPanel.Dock="Top"
                                    Margin="5,4,5,0">
                    <TextBlock Text="Broj gostiju:"
                                       Width="70"></TextBlock>
                    <TextBox Margin="5,0,0,0"
                                     HorizontalContentAlignment="Center"
                                     Text="{Binding BrojGostiju, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">
                        <i:Interaction.Triggers>
                            <i:EventTrigger EventName="GotFocus">
                                <i:InvokeCommandAction Command="{Binding IzabranTB_Command}" CommandParameter="BrojGostiju"></i:InvokeCommandAction>
                            </i:EventTrigger>
                        </i:Interaction.Triggers>
                    </TextBox>
                </DockPanel>
                <DockPanel DockPanel.Dock="Top"
                                    Margin="5,4,5,0">
                    <TextBlock Text="Gost:"
                                       Width="70"></TextBlock>
                    <TextBox Margin="5,0,0,0"
                             HorizontalContentAlignment="Center"
                                     Text="{Binding Gost, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">
                        <i:Interaction.Triggers>
                            <i:EventTrigger EventName="GotFocus">
                                <i:InvokeCommandAction Command="{Binding IzabranTB_Command}" CommandParameter="Gost"></i:InvokeCommandAction>
                            </i:EventTrigger>
                        </i:Interaction.Triggers>
                    </TextBox>
                </DockPanel>
                <DockPanel DockPanel.Dock="Top"
                                    Margin="5,4,5,0">
                    <TextBlock Text="Telefon:"
                                       Width="70"></TextBlock>
                    <TextBox Margin="5,0,0,0"
                             HorizontalContentAlignment="Center"
                                     Text="{Binding Telefon, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">
                        <i:Interaction.Triggers>
                            <i:EventTrigger EventName="GotFocus">
                                <i:InvokeCommandAction Command="{Binding IzabranTB_Command}" CommandParameter="Telefon"></i:InvokeCommandAction>
                            </i:EventTrigger>
                        </i:Interaction.Triggers>
                    </TextBox>
                </DockPanel>
                <Button BorderBrush="Transparent"
                                    Margin="5,5,5,0"
                                    DockPanel.Dock="Top"
                                    Command="{Binding NadjiSlobodanSto_Command}">
                    <Button.BitmapEffect>
                        <DropShadowBitmapEffect Color="#333333" Direction="360" ShadowDepth="0" Opacity="0.5">

                        </DropShadowBitmapEffect>
                    </Button.BitmapEffect>
                    <Button.Resources>
                        <Style TargetType="{x:Type Border}">
                            <Setter Property="CornerRadius" Value="3"></Setter>
                        </Style>
                    </Button.Resources>

                    <TextBlock Text="Pronađi gosta"
                                   Foreground="WhiteSmoke"
                                           VerticalAlignment="Center"
                                           HorizontalAlignment="Center"
                                   FontSize="14"
                                   TextWrapping="Wrap">
                    </TextBlock>
                    <Button.Style>
                        <Style TargetType="{x:Type Button}">
                            <Setter Property="Background" Value="#284b63"/>
                            <Setter Property="Template">
                                <Setter.Value>
                                    <ControlTemplate TargetType="{x:Type Button}">
                                        <Border Background="{TemplateBinding Background}">
                                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                        </Border>
                                    </ControlTemplate>
                                </Setter.Value>
                            </Setter>
                            <Style.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter Property="Background" Value="#427aa1"/>
                                </Trigger>
                            </Style.Triggers>
                        </Style>
                    </Button.Style>
                </Button>
                <Border Background="#284b63"
                        DockPanel.Dock="Top"
                        Height="20"
                        Margin="0,10,0,0"
                        >
                    <TextBlock Text="Izabrani stolovi"
                               Foreground="White"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="16"></TextBlock>
                </Border>
                <ListView DockPanel.Dock="Top"
                          Foreground="#284b63"
                          HorizontalContentAlignment="Center"
                          FontSize="14"
                          ItemsSource="{Binding IzabraniStolovi, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"></ListView>
            </DockPanel>
        </Border>
        <Border Grid.Column="1"
                BorderBrush="DimGray"
                Grid.RowSpan="2"
                BorderThickness="2,2,2,2"
                Background="LightGray">
            <DockPanel LastChildFill="False"
                   >
                <ScrollViewer HorizontalScrollBarVisibility="Hidden"
                              VerticalScrollBarVisibility="Hidden"
                              DockPanel.Dock="Top">
                    <DockPanel LastChildFill="False"
                               x:Name="dpSlobodniStolovi"
                               Margin="0,10,0,0">
                    </DockPanel>
                </ScrollViewer>
            </DockPanel>
        </Border>
